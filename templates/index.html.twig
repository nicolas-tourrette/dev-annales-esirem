    <!-- Main CSS-->
    <link href="/css/theme.css" rel="stylesheet" media="all">

    <script type="text/javascript">
        function cryptm(m, o, n) {
            if (!document.write) {
                return false;
            }
            else {
                var m; var o; var n;
                document.write('<a style="text-decoration: none;" href="' + 'mailto:' + m + '@' + o + '.' + n + '">' + m + '@' + o + '.' + n + ' <img src="https://svgsilh.com/svg/303617-4caf50.svg" width="14" height="14"></a>');
            }
        }
    </script>
    {% endblock %}
